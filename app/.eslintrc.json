{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "root": true,
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "globals": {
    "google": "readonly"
  },
  "overrides": [
    {
      "files": ["src/**"],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}
